<div class="{{classPrefix}}">
    <div class="{{classPrefix}}-content">
        {{> header}}
        <ul data-role="items">
            {{#each items}}
                <li data-role="item" class="{{../classPrefix}}-item">
                    <a href="javascript:void(0)" title="{{deptDesc}}">
                        <span class="suggest-avatar"><img width="25" height="25" src="https://work.alibaba-inc.com/photo/{{empId}}.jpg"></span>
                        <span class="suggest-empid">{{empId}}</span>
                        <span class="suggest-name">{{lastName}}</span>
                    </a>
                </li>
            {{/each}}
        </ul>
        {{> footer}}
    </div>
</div>
